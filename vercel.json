{
  "framework": null,
  "buildCommand": "node build.js && node generate-llms.js",
  "outputDirectory": "public",
  "functions": {
    "api/*.js": {
      "memory": 256,
      "maxDuration": 15
    }
  },
  "rewrites": [
    { "source": "/api/webhook", "destination": "/api/webhook.js" },
    { "source": "/api/feedback", "destination": "/api/feedback.js" },
    { "source": "/api/chat", "destination": "/api/chat.js" }
  ]
}
